{
  "author": "Narato",
  "classifications": [
    "Narato Libraries",
    "Configuration",
    "API"
  ],
  "name": "Web API template for using Narato Libraries stack",
  "tags": {
    "language": "C#"
  },
  "identity": "Narato.Libraries.WebAPI",
  "shortName": "naratowebapi",
  "sourceName": "Narato.Libraries.POC",
  "sources": [
    {
      "source": "./",
      "target": "./",
      "exclude": [
        "**/*.log",
        ".template.config/*",
        ".vs/**/*",
        ".git/**/*"
      ],
      "modifiers": [
        {
          "condition": "(!EnableExample)",
          "exclude": [
            "**/*Book*.cs",
            "**/*Author*.cs"
          ]
        },
        {
          "condition": "(EnableExample)",
          "exclude": [
            "**/.notempty"
          ]
        }
      ]
    }
  ],
  "preferNameDirectory": true,
  "symbols": {
    "EnableExample": {
      "type": "parameter",
      "dataType": "bool",
      "defaultValue": "false"
    }
  }
}